    "# Analytics Runner (Batch Processor)\n",
    "# analytics_runner.py does:\n",
    "# Loops through every .sql in src/kpi_sql/.\n",
    "# Executes them against the DB.\n",
    "# Writes each result to:\n",
    "# artifacts/kpi_out/*.csv\n",
    "# artifacts/kpi_out/README.md (Markdown preview)\n",
    "\n",
    "# Purpose:\n",
    "# ‚úÖ Quickly test and verify all KPI SQLs at once.\n",
    "# ‚úÖ Gives you ground-truth outputs for later validation.\n",
    "\n",
    "import os, glob\n",
    "import pandas as pd\n",
    "from dotenv import load_dotenv\n",
    "from sqlalchemy import create_engine, text\n",
    "\n",
    "load_dotenv()\n",
    "DB_URL = os.getenv(\"DATABASE_URL\")\n",
    "engine = create_engine(DB_URL, pool_pre_ping=True)\n",
    "\n",
    "os.makedirs(\"artifacts/kpi_out\", exist_ok=True)\n",
    "\n",
    "def run_sql(path):\n",
    "    sql = open(path).read()\n",
    "    with engine.connect() as con:\n",
    "        return pd.read_sql(text(sql), con)\n",
    "\n",
    "summary = []\n",
    "for p in sorted(glob.glob(\"src/kpi_sql/*.sql\")):\n",
    "    name = os.path.basename(p)\n",
    "    df = run_sql(p)\n",
    "    df.to_csv(f\"artifacts/kpi_out/{name}.csv\", index=False)\n",
    "    print(f\"{name}: {len(df)} rows\")\n",
    "    display(df.head())\n"
   ]
