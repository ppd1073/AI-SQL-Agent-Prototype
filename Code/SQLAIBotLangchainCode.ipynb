    "?9. Simple Visualization Hook (plot_bar)\n",
    "# You added a lightweight Matplotlib helper:\n",
    "# plot_bar(df, x_col=\"sales_class_desc\", y_col=\"total_invoiced\")\n",
    "# Takes the DataFrame, selects x/y columns, and plots a bar chart.\n",
    "# No explicit colors (compliance with matplotlib rules).\n",
    "\n",
    "# Purpose:\n",
    "# ‚úÖ Quick visual exploration of results without leaving Jupyter.\n",
    "# ‚úÖ Completes the ‚Äúanalytics loop‚Äù ‚Äî from natural language ‚Üí SQL ‚Üí insight ‚Üí plot.\n",
    "# --- Simple plotting helpers (Matplotlib, single-chart, no explicit colors) ---\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def plot_bar(df: pd.DataFrame, x_col: str, y_col: str, top: int = 10, title: str | None = None, rotate=45):\n",
    "    if x_col not in df.columns or y_col not in df.columns:\n",
    "        raise ValueError(f\"Columns not in df: {x_col}, {y_col}\")\n",
    "\n",
    "    d = df[[x_col, y_col]].copy()\n",
    "    # keep top-N by y descending\n",
    "    d = d.sort_values(y_col, ascending=False).head(top)\n",
    "\n",
    "    plt.figure(figsize=(10, 5))\n",
    "    plt.bar(d[x_col].astype(str), d[y_col].values)\n",
    "    plt.xticks(rotation=rotate, ha=\"right\")\n",
    "    if title:\n",
    "        plt.title(title)\n",
    "    plt.xlabel(x_col)\n",
    "    plt.ylabel(y_col)\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n"
   ]
