   "source": [
    "# üß† PHASE 2 ‚Äî Analytical Query Layer (Human-Written SQL)\n",
    "# 3. KPI SQL Files (src/kpi_sql/*.sql)\n",
    "# You hand-crafted six analytical SQL queries:\n",
    "# Revenue by Sales Agent\n",
    "# Jobs by Sales Class\n",
    "# Unshipped or Partial Shipments\n",
    "# Invoice Trends by Sales Class\n",
    "# Forecast vs Actual\n",
    "# Cost by Machine Type\n",
    "# Each file isolates one KPI query in pure SQL.\n",
    "\n",
    "# Purpose:\n",
    "# ‚úÖ Creates reusable analytical queries.\n",
    "# ‚úÖ Defines the ‚Äútruth‚Äù your AI will later imitate when generating SQL automatically.\n",
    "\n",
    "sql_files = {\n",
    "    \"revenue_by_agent.sql\": \"\"\"\n",
    "SELECT \n",
    "    sa.sales_agent_name,\n",
    "    SUM(i.invoice_amount) AS total_revenue\n",
    "FROM w_invoiceline_f AS i\n",
    "JOIN w_sales_agent_d AS sa\n",
    "  ON i.sales_agent_id = sa.sales_agent_id\n",
    "GROUP BY sa.sales_agent_name\n",
    "ORDER BY total_revenue DESC;\n",
    "\"\"\",\n",
    "    \"jobs_by_sales_class.sql\": \"\"\"\n",
    "SELECT \n",
    "    sc.sales_class_desc,\n",
    "    COUNT(j.job_id) AS total_jobs,\n",
    "    SUM(j.unit_price * j.quantity_ordered) AS estimated_booking_value\n",
    "FROM w_job_f AS j\n",
    "JOIN w_sales_class_d AS sc\n",
    "  ON j.sales_class_id = sc.sales_class_id\n",
    "GROUP BY sc.sales_class_desc\n",
    "ORDER BY estimated_booking_value DESC;\n",
    "\"\"\",\n",
    "    \"unshipped_or_partial.sql\": \"\"\"\n",
    "SELECT \n",
    "    j.job_id,\n",
    "    js.requested_quantity,\n",
    "    js.actual_quantity,\n",
    "    (js.requested_quantity - js.actual_quantity) AS qty_pending\n",
    "FROM w_job_shipment_f AS js\n",
    "JOIN w_sub_job_f AS sj\n",
    "  ON js.sub_job_id = sj.sub_job_id\n",
    "JOIN w_job_f AS j\n",
    "  ON sj.job_id = j.job_id\n",
    "WHERE js.actual_quantity < js.requested_quantity\n",
    "ORDER BY qty_pending DESC;\n",
    "\"\"\",\n",
    "    \"invoice_trends_by_class.sql\": \"\"\"\n",
    "SELECT \n",
    "    sc.sales_class_desc,\n",
    "    SUM(i.invoice_amount) AS total_invoiced,\n",
    "    COUNT(DISTINCT i.invoice_id) AS num_invoices\n",
    "FROM w_invoiceline_f AS i\n",
    "JOIN w_sales_class_d AS sc\n",
    "  ON i.sales_class_id = sc.sales_class_id\n",
    "GROUP BY sc.sales_class_desc\n",
    "ORDER BY total_invoiced DESC;\n",
    "\"\"\",\n",
    "    \"forecast_vs_actual.sql\": \"\"\"\n",
    "SELECT \n",
    "    t.time_year,\n",
    "    t.time_quarter,\n",
    "    t.time_month,\n",
    "    SUM(fs.actual_amount)  AS total_actual,\n",
    "    SUM(fs.forcast_amount) AS total_forecast,\n",
    "    (SUM(fs.actual_amount) - SUM(fs.forcast_amount)) AS variance\n",
    "FROM w_financial_summary_sales_f AS fs\n",
    "JOIN w_time_d AS t\n",
    "  ON fs.report_end_date_id = t.time_id\n",
    "GROUP BY t.time_year, t.time_quarter, t.time_month\n",
    "ORDER BY t.time_year, t.time_quarter, t.time_month;\n",
    "\"\"\",\n",
    "    \"cost_by_machine_type.sql\": \"\"\"\n",
    "SELECT \n",
    "    mt.machine_model,\n",
    "    SUM(fc.actual_labor_cost)    AS labor_cost,\n",
    "    SUM(fc.actual_machine_cost)  AS machine_cost,\n",
    "    SUM(fc.actual_material_cost) AS material_cost,\n",
    "    SUM(fc.actual_overhead_cost) AS overhead_cost,\n",
    "    SUM(fc.actual_labor_cost + fc.actual_machine_cost +\n",
    "        fc.actual_material_cost + fc.actual_overhead_cost) AS total_cost\n",
    "FROM w_financial_summary_cost_f AS fc\n",
    "JOIN w_machine_type_d AS mt\n",
    "  ON fc.machine_type_id = mt.machine_type_id\n",
    "GROUP BY mt.machine_model\n",
    "ORDER BY total_cost DESC;\n",
    "\"\"\"\n",
    "}\n",
    "\n",
    "for name, query in sql_files.items():\n",
    "    with open(f\"src/kpi_sql/{name}\", \"w\") as f:\n",
    "        f.write(query.strip() + \"\\n\")\n",
    "print(\"‚úÖ KPI SQL files created in src/kpi_sql/\")\n"
   ]
  },
