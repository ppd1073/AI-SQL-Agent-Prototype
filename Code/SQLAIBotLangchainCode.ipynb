   "source": [
    "# ü§ñ PHASE 3 ‚Äî AI Integration (LangChain & OpenAI)\n",
    "# 5. Code 1 ‚Äî ‚ÄúAI SQL Agent‚Äù (Chat-Style LangChain Agent)\n",
    "# Uses LangChain‚Äôs SQL Agent (create_sql_agent).\n",
    "# The model is guided by your schema and can call SQL tools.\n",
    "# Runs in verbose ‚Äúchain‚Äù mode, producing multi-step logs like\n",
    "# ‚ÄúAction: sql_db_list_tables ‚Ä¶ Action: sql_db_query ‚Ä¶‚Äù\n",
    "\n",
    "# Purpose:\n",
    "# ‚úÖ Full reasoning chain.\n",
    "# ‚ùå But too verbose and messy for Jupyter (it prints intermediate tool calls).\n",
    "\n",
    "# What it does under the hood:\n",
    "# Reads the schema through the agent tool.\n",
    "# Decides which table(s) to query.\n",
    "# Generates SQL.\n",
    "# Executes it automatically.\n",
    "# Returns the final answer (not necessarily the SQL itself).\n",
    "\n",
    "# -- Fixed LangChain SQL Agent: returns the executed SQL, not prose --\n",
    "import os, re, json\n",
    "import pandas as pd\n",
    "from dotenv import load_dotenv\n",
    "from sqlalchemy import create_engine, text\n",
    "from tabulate import tabulate\n",
    "\n",
    "from langchain_openai import ChatOpenAI\n",
    "from langchain_community.utilities import SQLDatabase\n",
    "from langchain.agents.agent_toolkits import create_sql_agent\n",
    "from langchain.agents import AgentExecutor\n",
    "\n",
    "load_dotenv()\n",
    "\n",
    "OPENAI_MODEL = os.getenv(\"OPENAI_MODEL\", \"gpt-4o-mini\")\n",
    "DB_URL = os.getenv(\"DATABASE_URL\")\n",
    "STATEMENT_TIMEOUT_MS = int(os.getenv(\"STATEMENT_TIMEOUT_MS\", \"4000\"))\n",
    "ROWS_PREVIEW = int(os.getenv(\"ROWS_PREVIEW\", \"50\"))\n",
    "\n",
    "# ---- DB + LC wrappers\n",
    "engine = create_engine(\n",
    "    DB_URL,\n",
    "    connect_args={\"options\": f\"-c statement_timeout={STATEMENT_TIMEOUT_MS}\"},\n",
    "    pool_pre_ping=True,\n",
    ")\n",
    "db = SQLDatabase.from_uri(DB_URL)\n",
    "llm = ChatOpenAI(model=OPENAI_MODEL, temperature=0)\n",
    "\n",
    "# ---- Load schema excerpt for grounding (optional but helpful)\n",
    "try:\n",
    "    schema_card_md = open(\"artifacts/schema_card.md\", \"r\", encoding=\"utf-8\").read()[:2500]\n",
    "except:\n",
    "    schema_card_md = \"(schema card missing)\"\n",
    "\n",
    "# ---- Safety helpers\n",
    "def ensure_select(sql: str):\n",
    "    s = sql.strip().lower()\n",
    "    if not s.startswith(\"select\"):\n",
    "        raise ValueError(\"Refusing non-SELECT SQL.\")\n",
    "    for bad in [\" update \", \" delete \", \" insert \", \" drop \", \" alter \", \" truncate \", \" create \"]:\n",
    "        if bad in s:\n",
    "            raise ValueError(\"Potentially destructive SQL detected.\")\n",
    "\n",
    "def strip_fences(s: str) -> str:\n",
    "    m = re.search(r\"```sql(.*?)```\", s, re.S|re.I)\n",
    "    if m: return m.group(1).strip()\n",
    "    m = re.search(r\"```(.*?)```\", s, re.S|re.I)\n",
    "    return m.group(1).strip() if m else s.strip()\n",
    "\n",
    "# ---- Create agent with intermediate steps enabled (so we can extract the tool's SQL)\n",
    "agent: AgentExecutor = create_sql_agent(\n",
    "    llm=llm,\n",
    "    db=db,\n",
    "    verbose=False,                       # silence tool chatter in Jupyter\n",
    "    return_intermediate_steps=True,      # <- critical to get the SQL back\n",
    ")\n",
    "\n",
    "# ---- Router rules to avoid summary-table confusion\n",
    "ROUTING_RULES = \"\"\"\n",
    "RULES (follow strictly):\n",
    "- If the user asks about \"invoice\", \"invoiced amount\", \"invoice totals\", or \"billing\",\n",
    "  USE table w_invoiceline_f (column invoice_amount), joining w_sales_class_d and/or w_sales_agent_d for names.\n",
    "  DO NOT use w_financial_summary_sales_f for invoiced totals.\n",
    "- If the user asks about \"forecast vs actual\", \"variance\", or time-based summaries,\n",
    "  USE w_financial_summary_sales_f joined to w_time_d (and other relevant dimensions).\n",
    "- Only generate a single safe SELECT query.\n",
    "- Prefer explicit column names and LIMIT where appropriate.\n",
    "\"\"\"\n",
    "\n",
    "SYSTEM_GUARD = f\"\"\"You are a PostgreSQL SQL assistant.\n",
    "{ROUTING_RULES}\n",
    "Schema excerpt:\n",
    "{schema_card_md}\n",
    "Return one safe SELECT for the user's question.\n",
    "\"\"\"\n",
    "\n",
    "def agent_nl_to_sql(question: str) -> str:\n",
    "    \"\"\"\n",
    "    Run the LC SQL agent, then extract the last SQL it sent to the SQL tool.\n",
    "    Fall back to parsing fenced SQL in the final output if needed.\n",
    "    \"\"\"\n",
    "    # Agent expects \"input\"; we prepend our guard + the question.\n",
    "    user_msg = f\"{SYSTEM_GUARD}\\nUser question: {question}\"\n",
    "\n",
    "    # Ask for intermediate steps so we can pull the SQL tool input.\n",
    "    result = agent.invoke({\"input\": user_msg})\n",
    "\n",
    "    # 1) Best source of truth: the last tool call the agent made to the SQL tool.\n",
    "    #    intermediate_steps is a list of (AgentAction, Observation).\n",
    "    steps = result.get(\"intermediate_steps\", [])\n",
    "    sql_candidates = []\n",
    "    for action, _obs in steps:\n",
    "        # Most SQL agents use a tool named \"sql_db_query\" or \"query_sql_db\"\n",
    "        # and they place the SQL in action.tool_input (string or dict).\n",
    "        if getattr(action, \"tool\", \"\") in {\"sql_db_query\", \"query_sql_db\"}:\n",
    "            ti = action.tool_input\n",
    "            # tool_input can be a dict like {\"query\": \"...\"} or a raw SQL string\n",
    "            if isinstance(ti, dict) and \"query\" in ti:\n",
    "                sql_candidates.append(ti[\"query\"])\n",
    "            elif isinstance(ti, str):\n",
    "                sql_candidates.append(ti)\n",
    "\n",
    "    if sql_candidates:\n",
    "        sql = sql_candidates[-1].strip()\n",
    "        sql = strip_fences(sql)\n",
    "        ensure_select(sql)\n",
    "        return sql\n",
    "\n",
    "    # 2) Fallback: sometimes the agent's final output embeds SQL in fences.\n",
    "    final_text = result.get(\"output\", \"\") or str(result)\n",
    "    sql = strip_fences(final_text)\n",
    "    ensure_select(sql)\n",
    "    return sql\n",
    "\n",
    "def run_sql(sql: str) -> pd.DataFrame:\n",
    "    with engine.connect() as con:\n",
    "        df = pd.read_sql(text(sql), con)\n",
    "    return df.head(ROWS_PREVIEW) if len(df) > ROWS_PREVIEW else df\n",
    "\n",
    "def summarize(question: str, sql: str, df: pd.DataFrame) -> str:\n",
    "    sample = df.to_dict(orient=\"records\")[:10]\n",
    "    prompt = f\"\"\"Summarize for a business audience (‚â§6 sentences).\n",
    "Question: {question}\n",
    "SQL: {sql}\n",
    "First rows JSON: {json.dumps(sample, ensure_ascii=False)}\"\"\"\n",
    "    return llm.invoke([{\"role\":\"user\",\"content\":prompt}]).content\n",
    "\n",
    "print(\"‚úÖ LangChain agent fixed and ready.\")\n"
   ]
  },
