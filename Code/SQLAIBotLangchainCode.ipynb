   "source": [
    "# --- Validation + Caching for NL‚ÜíSQL workflow ---\n",
    "# PHASE 4 ‚Äî Validation, Caching, and Visualization Layer\n",
    "# 8. Validation + Caching Block\n",
    "# This introduced several enhancements for reliability and speed.\n",
    "# Components:\n",
    "# Function\tRole\n",
    "# _sql_cache_key(sql)\tCreates a SHA-1 hash from SQL text.\n",
    "# cache_get() / cache_put()\tReads/writes cached DataFrames (Parquet/CSV).\n",
    "# validate_df()\tChecks for missing/empty/nonnumeric columns, nulls, row count.\n",
    "# ask()\tOne-line end-to-end wrapper: NL ‚Üí SQL ‚Üí DB ‚Üí Validation ‚Üí Summary, using caching under the hood.\n",
    "\n",
    "# Purpose:\n",
    "# ‚úÖ Avoids re-querying DB for repeated questions.\n",
    "# ‚úÖ Warns when AI-generated SQL returns unexpected structure.\n",
    "# ‚úÖ Makes notebook interaction stable and explainable.\n",
    "\n",
    "\n",
    "    \n",
    "import os, hashlib, pandas as pd\n",
    "from pathlib import Path\n",
    "\n",
    "# Folders\n",
    "Path(\"artifacts/cache\").mkdir(parents=True, exist_ok=True)\n",
    "\n",
    "def _sql_cache_key(sql: str) -> str:\n",
    "    return hashlib.sha1(sql.encode(\"utf-8\")).hexdigest()[:16]\n",
    "\n",
    "def _cache_paths(sql: str):\n",
    "    key = _sql_cache_key(sql)\n",
    "    root = Path(\"artifacts/cache\") / key\n",
    "    return {\n",
    "        \"parquet\": root.with_suffix(\".parquet\"),\n",
    "        \"csv\": root.with_suffix(\".csv\"),\n",
    "        \"meta\": root.with_suffix(\".meta.txt\"),\n",
    "        \"key\": key,\n",
    "    }\n",
    "\n",
    "def cache_get(sql: str) -> pd.DataFrame | None:\n",
    "    paths = _cache_paths(sql)\n",
    "    if paths[\"parquet\"].exists():\n",
    "        try:\n",
    "            return pd.read_parquet(paths[\"parquet\"])\n",
    "        except Exception:\n",
    "            pass\n",
    "    if paths[\"csv\"].exists():\n",
    "        try:\n",
    "            return pd.read_csv(paths[\"csv\"])\n",
    "        except Exception:\n",
    "            pass\n",
    "    return None\n",
    "\n",
    "def cache_put(sql: str, df: pd.DataFrame):\n",
    "    paths = _cache_paths(sql)\n",
    "    try:\n",
    "        df.to_parquet(paths[\"parquet\"], index=False)\n",
    "    except Exception:\n",
    "        df.to_csv(paths[\"csv\"], index=False)\n",
    "    with open(paths[\"meta\"], \"w\", encoding=\"utf-8\") as f:\n",
    "        f.write(sql)\n",
    "\n",
    "def validate_df(\n",
    "    df: pd.DataFrame,\n",
    "    required_cols: list[str] | None = None,\n",
    "    value_cols: list[str] | None = None,\n",
    "    max_rows_warn: int = 200000\n",
    ") -> list[str]:\n",
    "    \"\"\"\n",
    "    Returns a list of warnings. Empty list => passes basic checks.\n",
    "    \"\"\"\n",
    "    warns: list[str] = []\n",
    "\n",
    "    if df is None or df.empty:\n",
    "        return [\"No rows returned.\"]\n",
    "\n",
    "    # Column existence\n",
    "    if required_cols:\n",
    "        missing = [c for c in required_cols if c not in df.columns]\n",
    "        if missing:\n",
    "            warns.append(f\"Missing expected columns: {missing}\")\n",
    "\n",
    "    # Basic types/NA checks for value columns\n",
    "    if value_cols:\n",
    "        for c in value_cols:\n",
    "            if c in df.columns:\n",
    "                if not pd.api.types.is_numeric_dtype(df[c]):\n",
    "                    warns.append(f\"Column '{c}' is not numeric (dtype={df[c].dtype}).\")\n",
    "                null_ratio = df[c].isna().mean()\n",
    "                if null_ratio > 0.05:\n",
    "                    warns.append(f\"Column '{c}' has {null_ratio:.1%} missing values.\")\n",
    "                if df[c].abs().sum() == 0:\n",
    "                    warns.append(f\"Sum of '{c}' is zero; check filters/joins.\")\n",
    "            else:\n",
    "                warns.append(f\"Value column '{c}' not found in result.\")\n",
    "\n",
    "    # Size guardrail\n",
    "    if len(df) > max_rows_warn:\n",
    "        warns.append(f\"Large result: {len(df):,} rows. Consider narrowing the query.\")\n",
    "\n",
    "    return warns\n",
    "\n",
    "def ask(question: str, required_cols=None, value_cols=None, preview=10, summarize_answer=True):\n",
    "    \"\"\"\n",
    "    Full pipeline: NL -> SQL -> (cache) -> DataFrame -> validation -> summary\n",
    "    \"\"\"\n",
    "    sql = nl_to_sql_direct(question)\n",
    "    print(\"SQL:\\n\", sql)\n",
    "\n",
    "    df_cached = cache_get(sql)\n",
    "    if df_cached is not None:\n",
    "        df = df_cached\n",
    "        from_cache = True\n",
    "    else:\n",
    "        df = run_sql(sql)\n",
    "        cache_put(sql, df)\n",
    "        from_cache = False\n",
    "\n",
    "    print(f\"\\nRows: {len(df)}  |  From cache: {from_cache}\")\n",
    "    display(df.head(preview))\n",
    "\n",
    "    warns = validate_df(df, required_cols=required_cols, value_cols=value_cols)\n",
    "    if warns:\n",
    "        print(\"\\n‚ö†Ô∏è Validation warnings:\")\n",
    "        for w in warns:\n",
    "            print(\" -\", w)\n",
    "    else:\n",
    "        print(\"\\n‚úÖ Validation checks passed.\")\n",
    "\n",
    "    if summarize_answer:\n",
    "        print(\"\\nSummary:\")\n",
    "        print(summarize(question, sql, df))\n",
    "\n",
    "    return df, sql\n"
   ]
  },
