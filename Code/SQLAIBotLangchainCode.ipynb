   "source": [
    "#1. Connection Setup (SQLAlchemy + dotenv)\n",
    "# You started with a simple script that:\n",
    "# Loads environment variables (.env) for credentials.\n",
    "# Builds a PostgreSQL connection string (postgresql+psycopg2://user:password@host:port/dbname).\n",
    "# Tests the connection using SELECT current_database(), current_user.\n",
    "\n",
    "# Purpose:\n",
    "# ‚úÖ Confirms that Python ‚Üî PostgreSQL connection works.\n",
    "# ‚úÖ Secures credentials using .env instead of hardcoding.\n",
    "    \n",
    "import os\n",
    "os.environ[\"OPENAI_API_KEY\"] = \""\n",
    "os.environ[\"OPENAI_MODEL\"] = \"gpt-4o-mini\"\n",
    "os.environ[\"DATABASE_URL\"] = \"postgresql+psycopg2://postgres:password@localhost:5432/CapstoneProject1\"\n",
    "os.environ[\"STATEMENT_TIMEOUT_MS\"] = \"4000\"\n",
    "os.environ[\"ROWS_PREVIEW\"] = \"50\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "a11ca933-ecff-47f1-bd80-817d5edb6889",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 2. Schema Card Generator (schema_card.py)\n",
    "# You created a script that:\n",
    "# Queries information_schema.columns to list all tables, columns, and datatypes.\n",
    "# Writes:\n",
    "# artifacts/schema_card.md (Markdown summary)\n",
    "# artifacts/schema_card.json (structured JSON)\n",
    "\n",
    "# Purpose:\n",
    "# ‚úÖ Produces a map of your database structure (tables + columns).\n",
    "# ‚úÖ This ‚Äúschema card‚Äù later helps the AI know what tables and fields exist.\n",
    "\n",
    "import json\n",
    "from dotenv import load_dotenv\n",
    "from sqlalchemy import create_engine, text\n",
    "\n",
    "load_dotenv()\n",
    "\n",
    "# Resolve DB URL: prefer DATABASE_URL, else compose from parts\n",
    "env_url = os.getenv(\"DATABASE_URL\")\n",
    "\n",
    "PG_USER = os.getenv(\"PGUSER\", \"postgres\")\n",
    "PG_PW   = os.getenv(\"PGPASSWORD\", \"password\")\n",
    "PG_HOST = os.getenv(\"PGHOST\", \"localhost\")\n",
    "PG_PORT = os.getenv(\"PGPORT\", \"5432\")  # change to 5432 if that's your server\n",
    "PG_DB   = os.getenv(\"PGDATABASE\", \"CapstoneProject1\")\n",
    "\n",
    "composed_url = f\"postgresql+psycopg2://{PG_USER}:{PG_PW}@{PG_HOST}:{PG_PORT}/{PG_DB}\"\n",
    "DB_URL = env_url or composed_url\n",
    "\n",
    "STATEMENT_TIMEOUT_MS = int(os.getenv(\"STATEMENT_TIMEOUT_MS\", \"4000\"))\n",
    "\n",
    "# Safety: show masked URL for diagnostics\n",
    "safe_url = DB_URL.replace(PG_PW, \"*****\") if PG_PW and DB_URL else str(DB_URL)\n",
    "print(\"Using DB URL ->\", safe_url)\n",
    "\n",
    "engine = create_engine(\n",
    "    DB_URL,\n",
    "    connect_args={\"options\": f\"-c statement_timeout={STATEMENT_TIMEOUT_MS}\"},\n",
    "    pool_pre_ping=True,\n",
    ")\n",
    "\n",
    "SQL = \"\"\"\n",
    "SELECT c.table_name,\n",
    "       c.column_name,\n",
    "       c.data_type\n",
    "FROM information_schema.columns c\n",
    "WHERE c.table_schema = 'public'\n",
    "ORDER BY c.table_name, c.ordinal_position;\n",
    "\"\"\"\n",
    "\n",
    "card = {}\n",
    "try:\n",
    "    with engine.connect() as con:\n",
    "        rows = con.execute(text(SQL)).fetchall()\n",
    "        for t, col, dt in rows:\n",
    "            card.setdefault(t, []).append({\"column\": col, \"type\": dt})\n",
    "except Exception as e:\n",
    "    print(\"Failed to introspect schema:\", type(e).__name__, str(e))\n",
    "    print(\"Tried URL (masked):\", safe_url)\n",
    "    raise\n",
    "\n",
    "# Write markdown & json for humans + agents\n",
    "md_lines = []\n",
    "for t, cols in card.items():\n",
    "    md_lines.append(f\"### {t}\")\n",
    "    for x in cols:\n",
    "        md_lines.append(f\"- {x['column']} : {x['type']}\")\n",
    "    md_lines.append(\"\")\n",
    "\n",
    "os.makedirs(\"artifacts\", exist_ok=True)\n",
    "with open(\"artifacts/schema_card.md\", \"w\", encoding=\"utf-8\") as f:\n",
    "    f.write(\"\\n\".join(md_lines))\n",
    "with open(\"artifacts/schema_card.json\", \"w\", encoding=\"utf-8\") as f:\n",
    "    json.dump(card, f, indent=2, ensure_ascii=False)\n",
    "\n",
    "print(f\"Wrote artifacts/schema_card.md and artifacts/schema_card.json ({len(card)} tables)\")\n"
   ]
  },
